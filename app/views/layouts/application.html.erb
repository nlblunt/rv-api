<!DOCTYPE html>
<html>
  <head>
    <title>RvApi</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="nabvarSupportedContent">
        <ul class="navbar-nav">
          <li class="nav-item">
          <%= link_to 'New RV Parks', admin_rv_path , :class => "nav-link" %>
          </li>
        </ul>
        <ul class="navbar-nav">
          <li class="nav-item">
          <%= link_to 'All RV Parks', admin_rvall_path , :class => "nav-link" %>
          </li>
        </ul>
        <ul class="navbar-nav">
          <li class="nav-item">
          <%= link_to 'Requested Info RV Parks', admin_rvrequested_path , :class => "nav-link" %>
          </li>
        </ul>
        <ul class="navbar-nav">
          <li class="nav-item">
          <%= link_to 'Bulk Update', admin_bulkupdate_path, :class => "nav-link" %>
          </li>
        </ul>
        <ul class="navbar-nav">
          <li class="nav-item">
          <%= link_to 'Chamber', chambers_path, :class => "nav-link" %>
          </li>
        </ul>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
